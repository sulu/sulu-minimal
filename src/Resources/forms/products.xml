<?xml version="1.0" ?>
<properties xmlns="http://schemas.sulu.io/template/template"
            xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
            xsi:schemaLocation="http://schemas.sulu.io/template/template http://schemas.sulu.io/template/template-1.0.xsd">
    <property name="code" type="text_line" mandatory="true" colspan="3">
        <meta>
            <title lang="en">Code</title>
            <title lang="de">Code</title>
        </meta>
    </property>

    <property name="name" type="text_line" mandatory="true" colspan="9">
        <meta>
            <title lang="en">Name</title>
            <title lang="de">Name</title>
        </meta>
    </property>

    <property name="description" type="text_area">
        <meta>
            <title lang="en">Description</title>
            <title lang="de">Beschreibung</title>
        </meta>
    </property>
</properties>
